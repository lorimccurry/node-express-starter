{
  "name": "node-starter",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "production": "NODE_ENV=production NODE_PATH=./dist node dist/server.js",
    "build": "tsc -p .",
    "dev-tsc": "tsc --watch --preserveWatchOutput",
    "ts-node": "ts-node ./src/server.ts",
    "lint": "eslint src --ext js,ts",
    "lint-fix": "eslint --fix src --ext js,ts",
    "test": "dotenv -e .env.test jest --coverage",
    "test:watch": "dotenv -e .env.test jest --watch",
    "test:server": "dotenv -e .env.test nodemon src/server.ts",
    "generate_test": "dotenv -e .env.test npx prisma generate",
    "db_push_test": "dotenv -e .env.test npx prisma db push",
    "pretty": "prettier --write 'src/**/*.ts'",
    "prepare": "is-ci || husky install"
  },
  "keywords": [],
  "author": "Lori McCurry",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie": "^0.4.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.21",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.10.0",
    "eslint-plugin-jest": "^26.1.1",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.5",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "prisma": "^3.11.0",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@prisma/client": "^3.11.0",
    "bcrypt": "^5.0.1",
    "cookie": "^0.4.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "dotenv-cli": "^5.1.0",
    "express": "^4.17.3",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "winston": "^3.6.0"
  }
}
